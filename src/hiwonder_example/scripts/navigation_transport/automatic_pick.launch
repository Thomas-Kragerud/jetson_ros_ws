<?xml version="1.0"?>
<!--自动拾取-->
<launch>
    <arg name="debug"               default="false"/>
    <arg name="place_without_color" default="true"/>
    <arg name="broadcast"           default="false"/>

    <include file="$(find hiwonder_slam)/launch/include/hiwonder_robot.launch">
        <arg name="master_name" value="/"/>
        <arg name="robot_name"  value="/"/>
        <arg name="set_pose"    value="false"/>
    </include>

    <node name="automatic_pick" pkg="hiwonder_example" type="automatic_pick.py" output="screen">
        <param name="debug"                 value="$(arg debug)"/>
        <param name="broadcast"             value="$(arg broadcast)"/>
        <param name="place_without_color"   value="$(arg place_without_color)"/>
        <rosparam file="$(find hiwonder_example)/config/automatic_pick_roi.yaml"   command="load"/>
    </node>
</launch>
