<?xml version="1.0"?>
<launch>
    <!--底盘驱动(chassis driver)-->
    <include file="$(find hiwonder_controller)/launch/hiwonder_controller.launch"/>
    <include file="$(find hiwonder_peripherals)/launch/depth_cam.launch"/>

    <node pkg="hiwonder_example" type="hand_trajectory_node.py" name="hand_trajectory" output="screen">
        <param name="start" value="true"/>
    </node>

    <node pkg="hiwonder_example" type="hand_gesture_control_node.py" name="hand_gesture_control_node" output="screen"/>
</launch>
