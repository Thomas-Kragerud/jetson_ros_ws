<?xml version="1.0"?>
<!--颜色分拣-->
<launch>
    <arg name="start"   default="true"/>
    <arg name="debug"   default="false"/>
    <include file="$(find hiwonder_example)/scripts/color_detect/color_detect_node.launch"/>
    <include file="$(find hiwonder_controller)/launch/hiwonder_controller.launch"/>
    <node pkg="hiwonder_example" type="color_sorting_node.py" name="color_sorting" output="screen">
        <rosparam file="$(find hiwonder_example)/config/color_sorting_roi.yaml"   command="load"/>
        <param name="start" value="$(arg start)"/>
        <param name="debug" value="$(arg debug)"/>
    </node>
</launch>
